#! /bin/sh

MY_TCC=@TCC@
MY_N=@NN@
MY_P=@P@
MY_C=@C@
MY_V=@V@
MY_K=@K@
MY_D=@D@
MY_A=@A@

cd $MY_D
t=$(date)
echo "$t\n"
echo "srun --distribution=cyclic -N$MY_N -n$MY_TCC --distribution=cyclic $MY_K -l --nproducers=$MY_P --nconsumers=$MY_C --value-size=$MY_V --cons-acc-count=$MY_A\n"

sleep 3
srun -N$MY_N -n$MY_TCC --distribution=cyclic $MY_K -l --nproducers=$MY_P --nconsumers=$MY_C --value-size=$MY_V --cons-acc-count=$MY_A

t=$(date)
echo "$t \n"

